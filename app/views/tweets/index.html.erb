<p id="notice"><%= notice %></p>

<h1>Tweets</h1>

<section id="tweet_form">
  <%= render 'form', tweet: Tweet.new %>
</section>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id='tweets'>
    <%= render @tweets %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweet', new_tweet_path %>
